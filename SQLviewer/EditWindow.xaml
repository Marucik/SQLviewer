<Window x:Class="SQLviewer.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLviewer"
        mc:Ignorable="d"
        Title="EditWindow" Height="250" Width="800">
    <Grid>
        <TextBox x:Name="server_address" HorizontalAlignment="Left" Margin="65,80,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" Height="35"/>
        <TextBox x:Name="login" HorizontalAlignment="Left" Margin="293,80,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="169" Height="36"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Margin="528,80,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" Height="36"/>
        <Button Content="Edit" HorizontalAlignment="Left" Margin="293,161,0,0" VerticalAlignment="Top" Height="25" Width="138" Click="Button_Click"/>
        <TextBox x:Name="port" HorizontalAlignment="Left" Margin="708,80,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="53" Height="25"/>

    </Grid>
</Window>
